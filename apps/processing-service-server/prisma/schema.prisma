datasource db {
  provider = "mongodb"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model ProcessedPost {
  createdAt        DateTime  @default(now())
  id               String    @id @default(auto()) @map("_id") @db.ObjectId
  originalPostId   String?
  processedContent String?
  processingDate   DateTime?
  updatedAt        DateTime  @updatedAt
}
